{
    "name": "laravel/webportal",
    "description": "Webportal for Addessa Corporation.",
    "keywords": ["addessa", "webportal"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "anhskohbo/no-captcha": "^3.1",
        "barryvdh/laravel-dompdf": "^0.9",
        "doctrine/dbal": "^2.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "~6.0",
        "karlmonson/laravel-ping": "^1.0",
        "koolreport/bootstrap3": "^1.1",
        "koolreport/bootstrap4": "^1.1",
        "koolreport/core": "^4.0",
        "koolreport/laravel": "^1.6",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "maatwebsite/excel": "~2.1.0",
        "phpoffice/phpspreadsheet": "^1.24",
        "spatie/laravel-permission": "^2.17",
        "tymon/jwt-auth": "^1.0",
        "uxweb/sweet-alert": "^1.4",
        "yajra/laravel-datatables-editor": "^1.6",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
